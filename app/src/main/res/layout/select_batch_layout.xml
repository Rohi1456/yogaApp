<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="10dp">
    <!-- Project Selection Layout item while Create Task -->
    <appliedsyntax.io.yoga.customfonts.MyTextView_Roboto_Light
        android:textColor="#0d0d0d"
        android:id="@+id/select_BatchTVID"
        android:layout_width="match_parent"
        android:singleLine="true"
        android:ellipsize="end"
        android:gravity="left|start"
        android:layout_alignParentLeft="true"
        android:drawableRight="@drawable/ic_select"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@id/select_BatchTVID"
        android:layout_toStartOf="@id/select_BatchTVID"
        />


</RelativeLayout>
